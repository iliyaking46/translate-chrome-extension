{
  "name": "Translate app",
  "description": "Easy translate extension from EN to RU and backwards",
  "version": "1.0.3",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt + Q",
        "mac": "MacCtrl + Q"
      },
      "description": "Open translator"
    }
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "activeTab",
    "https://translate.googleapis.com/**"
  ]
}
